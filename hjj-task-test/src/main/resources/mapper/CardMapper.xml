<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="generator.mapper.CardMapper">

    <resultMap id="BaseResultMap" type="generator.domain.Card">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="userId" column="user_id" jdbcType="INTEGER"/>
            <result property="cardId" column="card_id" jdbcType="INTEGER"/>
            <result property="cardName" column="card_name" jdbcType="VARCHAR"/>
            <result property="cardOverage" column="card_overage" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,user_id,card_id,
        card_name,card_overage
    </sql>
</mapper>
